/* list.wxss */
@import '/resource/style/font-awesome.min.wxss';
.title{
  width: 100%;height: 40px;line-height: 40px;text-align: center;
  background-color: #fff;
}
.list-box{
  padding: 0 10px;margin-top: 7px;background-color: #fff;
}
.list-box .list-tit{
  width: 100%;
  height: 40px;line-height: 40px;border-bottom: 1px solid #eee;
  font-size: 14px;color: #333;position: relative;
}
.list-box .list-tit i{
  position: absolute;right: 5px;top:12px;font-size: 16px;
  color: #999;
}