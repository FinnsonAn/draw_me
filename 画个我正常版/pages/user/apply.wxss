/* pages/user/apply.wxss */
.banner{
  width: 100%;height: 260rpx;
}
.banner image{
  width: 100%;height: 260rpx;
}
.inp{
  width: 94%;margin: 0 auto;height: 50px;margin-top: 10px;border-radius:5px;background-color: #fff;border: 1px solid #ddd;
}
.inp text{
  float: left;margin-left: 10px;font-size: 14px;font-weight: bold;color: #666;line-height: 50px;
}
.inp  input{
  float:right;margin-right: 10px;line-height: 30px;font-size: 16px;text-align: right;height: 30px;margin-top: 10px;width: 350rpx;
}
.upload{
  width: 100%;height: auto;
}
.upload text{
  display: block;width: 94%;margin: 0 auto;font-size: 14px;font-weight: bold;color: #333;line-height: 50px;
}
.upload .w-list{
  width: 100%;display: table;
}
.upload .w-list view{
  width: 320rpx;height: 320rpx;float: left;margin-left: 22rpx;border: 1rpx solid #ddd;background-color: #fff;padding: 10rpx;margin-bottom: 22rpx;
}
.upload .w-list view image{
  width: 320rpx;height: 320rpx;
}
.up_but{
  height: 80px;
}
.up_but button{
  width: 92%;margin: 20px auto;background: #642ccd;color: #fff;
}